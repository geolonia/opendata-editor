{
  "name": "@geolonia/opendata-editor-web",
  "version": "1.1.1",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ode": "./dist/bin/cli.js"
  },
  "exports": {
    "./cli": {
      "import": {
        "default": "./dist/bin/lib.js",
        "types": "./dist/bin/lib.d.ts"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:cli",
    "build:lib": "vite build",
    "build:cli": "tsc --project ./tsconfig.cli.json && chmod +x ./dist/bin/cli.js",
    "test": "playwright test",
    "postinstall": "if npm list @playwright/test; then playwright install; fi"
  },
  "dependencies": {
    "@geolonia/opendata-editor": "^1.1.1",
    "@vitejs/plugin-react": "^4.0.4",
    "minimist": "^1.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.3",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "looks-same": "^9.0.0",
    "typescript": "^5.1.6",
    "undici": "^6.0.0"
  }
}
